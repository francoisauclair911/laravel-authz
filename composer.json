{
    "name": "casbin/laravel-authz",
    "keywords": [
        "laravel",
        "casbin",
        "permission",
        "access-control",
        "authorization",
        "rbac",
        "acl",
        "abac",
        "authz"
    ],
    "description": "An authorization library that supports access control models like ACL, RBAC, ABAC in Laravel. ",
    "authors": [
        {
            "name": "TechLee",
            "email": "techlee@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.5|~6.0|~7.0|~8.0",
        "casbin/casbin": "~3.1",
        "casbin/psr3-bridge": "^1.1",
        "guzzlehttp/guzzle": "^7.3|~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0|~9.0",
        "php-coveralls/php-coveralls": "^2.4",
        "mockery/mockery": "^1.0",
        "laravel/laravel": "~5.5|~6.0|~7.0|~8.0"
    },
    "autoload": {
        "psr-4": {
            "Lauthz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lauthz\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lauthz\\LauthzServiceProvider"
            ],
            "aliases": {
                "Enforcer": "Lauthz\\Facades\\Enforcer"
            }
        }
    }
}